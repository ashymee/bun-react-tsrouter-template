{
  "name": "bun-react-tsrouter-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/server.tsx",
  "module": "src/server.tsx",
  "scripts": {
    "gen": "tsr generate",
    "watch": "tsr watch",
    "hot": "bun --hot src/server.tsx",
